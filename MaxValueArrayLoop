import java.util.*;
class Main
{
    public static void main(String args[])
    {
        int arr[]={10,20,1,30,100,10,20,50,0,-5};
        int len=arr.length;
        int max=arr[0];
        for(int i=0;i<len;i++)
        {
            if(max<arr[i])
            {
                max=arr[i];
            }
        }
        System.out.println(max);
        
    }
}
