import java.util.*;
class Main
{
    public static void main(String args[])
    {
        int arr[]={0,100,10,20,1,30,100,10,20,50,0,-5};
        int min=arr[0];
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]<min)
            {
                min=arr[i];
            }
        }
        System.out.println(min);
       
        
    }
}
